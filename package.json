    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
